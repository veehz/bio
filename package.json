{
  "name": "bio",
  "version": "1.0.0",
  "description": "Biology Revision",
  "scripts": {
    "build": "rm -r docs & node preprocess.js --minify --prefetch",
    "dev": "concurrently --kill-others \"cd docs && live-server --no-browser\" \"when-changed -r src node preprocess.js --development --prefetch\"",
    "preview": "cd docs && live-server",
    "deploy": "npm run build && node gh-pages.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/veehz/bio.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/veehz/bio/issues"
  },
  "subpage": "bio",
  "homepage": "https://veehz.github.io/bio/",
  "dependencies": {
    "clean-css": "^5.3.1",
    "html-minifier-terser": "^7.0.0",
    "katex": "^0.16.2",
    "preprocess": "^3.2.0",
    "sitemap": "^7.1.1"
  },
  "devDependencies": {
    "@compodoc/live-server": "^1.2.3",
    "concurrently": "^7.5.0",
    "gh-pages": "^4.0.0"
  }
}
